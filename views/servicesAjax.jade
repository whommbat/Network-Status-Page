script.
    $(function (){
        $("[rel=tooltip]").tooltip();
    });

table.center
    //- $sabnzbdXML = simplexml_load_file('http://'.$sab_ip.':'.$sab_port.'/api?mode=qstatus&output=xml&apikey='.$sabnzbd_api);
    //- 
    //- if (($sabnzbdXML->state) == 'Downloading'):
    //- 	$timeleft = $sabnzbdXML->timeleft;
    //- 	$sabTitle = 'SABnzbd ('.$timeleft.')';
    //- else:
    //- 	$sabTitle = 'SABnzbd';
    //- endif;
    for service in services
        tr
            td(style="text-align: right; padding-right:5px;").exoextralight #{service.name}
            td(style="text-align: left;")
                if(service.url)
                    a(href=service.url, style="width: 62px;", class='btn-' + (service.status ? 'success' : 'warning'), disabled).btn.btn-xs
                        i.icon-white(class=('icon-' + (service.status ? 'ok' : 'remove')))
                        |  #{service.status ? 'Online' : 'Offline'}
                else
                    button(style="width: 62px;", class='btn-' + (service.status ? 'success' : 'warning'), disabled).btn.btn-xs
                        i.icon-white(class=('icon-' + (service.status ? 'ok' : 'remove'))) #{service.status ? 'Online' : 'Offline'}
